import java.util.Scanner;
public class ForEx {

	public static void main(String[] args) {
		int num, i;
		Scanner in = new Scanner(System.in);
		System.out.println("enter any value: ");
		num = in.nextInt();
		for(i=1;i<=10;i++)
		{
			System.out.println(num+ " * "+i+ " = " +(num*i));
		}
				

	}

}
